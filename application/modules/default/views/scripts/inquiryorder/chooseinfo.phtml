			{alert('请上传营业执照');window.location.href="#div_invoice_box";return;}
			 if(annexurl2==1)
			{alert('请上传税务登记证');window.location.href="#div_invoice_box";return;}
		}
	}
	//产品Item
	var part_id       = new Array();
	var part_id_obj=document.getElementsByName("part_id");
	for(var i=0;i<part_id_obj.length;i++)
	{
		part_id[i] = part_id_obj[i].value;
    }
	var buynum       = new Array();
	var buynum_obj=document.getElementsByName("buynum");
	for(var i=0;i<buynum_obj.length;i++)
	{
		buynum[i] = buynum_obj[i].value;
    }
	var customer_material       = new Array();
	var customer_material_obj=document.getElementsByName("customer_material");
	for(var i=0;i<customer_material_obj.length;i++)
	{
		customer_material[i] = customer_material_obj[i].value;
    }
	var needs_time = new Array();
	var needs_time_obj=document.getElementsByName("needs_time");
    for(var i=0;i<needs_time_obj.length;i++){
		needs_time[i] = needs_time_obj[i].value;
    }
	
	var subtmp = document.getElementById("orderbtn");
	subtmp.innerHTML = '<a href="javascript:;" class="button">订单提交中...</a>';

	var multiple = <?php echo json_encode($this->multiple);?>;
	
    var spot_part  = new Array();

	$.ajax({
            url: '/inquiryorder/handle',
            data: {'key':'<?=$this->key?>','items':'<?=$this->itemstr?>','delivery_type':delivery_type,'delivery_type_4_array':delivery_type_4_array,'multiple':multiple,'addressid':addressid,'invoiceaddress':invoiceaddress,'invoiceid':invoiceid,'paymenttype':paymenttype,'needinvoice':needinvoicevalue,'remark':remark,'shipoder_salesnumber':shipoder_salesnumber,'part_id':part_id,'buynum':buynum,'customer_material':customer_material,'needs_time':needs_time},
            type: 'post',
