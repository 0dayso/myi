	var invoiceid    = document.getElementById("invoiceid").value;
	var pyobj        = document.getElementsByName("paymenttype");
	var needremark = document.getElementById("needremark").checked;
	//合并订单
	var shipoder_salesnumber = '';
	var needshipoder_obj=document.getElementById("needshipoder");
	if(needshipoder_obj.checked){
		var shipoder_salesnumber_obj = document.getElementsByName("shipoder_salesnumber");
		for (var i=shipoder_salesnumber_obj.length-1;i>=0;i--){
		   if(shipoder_salesnumber_obj[i].checked)  shipoder_salesnumber = shipoder_salesnumber_obj[i].value;
        }
		if(shipoder_salesnumber==''){
		   alert('请选择合并发货的订单');window.location.href="#";return;
		}
	}
	var remark = '';
	if(needremark){
	   remark  = document.getElementById("remark").value;
	   if(remark=='') {alert('请填写订单备注');document.getElementById("remark").focus();return;}
	}
	var delivery_type_obj = document.getElementsByName("delivery_type");
	
	for (var i=delivery_type_obj.length-1;i>=0;i--)
	{
		if(delivery_type_obj[i].checked)  delivery_type = delivery_type_obj[i].value;
    }
    if(delivery_type=='') {alert('请选择配送方式');window.location.href="#";return;}
	
	if(delivery_type==1 || delivery_type==2){
		if(addressid=='') {alert('请选择收货人信息');window.location.href="#div_shipment";return;}
	}else if(delivery_type==4){
	    //客户提供账号
		delivery_type_4_array[0] = $("#address_4").val();
		if(delivery_type_4_array[0]==''){alert('请填写收货地址');$("#address_4").focus();return;}
		delivery_type_4_array[1] = $("#name_4").val();
		if(delivery_type_4_array[1]==''){alert('请填写收货人');$("#name_4").focus();return;}
		delivery_type_4_array[2] = $("#tel_4").val();
		if(delivery_type_4_array[2]==''){alert('请填写联系方式');$("#tel_4").focus();return;}
		delivery_type_4_array[3] = $("#express_name_4").val();
		if(delivery_type_4_array[3]==''){alert('请填写快递商');$("#express_name_4").focus();return;}
		delivery_type_4_array[4] = $("#express_account_4").val();
		if(delivery_type_4_array[4]==''){alert('请填写快递账号');$("#express_account_4").focus();return;}
		delivery_type_4_array[5] = $("#companyname_4").val();
		if(delivery_type_4_array[5]==''){alert('请填写公司名称');$("#companyname_4").focus();return;}
	}
	//发票地址
	var invoiceaddress = '';
	var invaddobj        = document.getElementsByName("invoiceaddressradio");
	for (var i=invaddobj.length-1;i>=0;i--)
	{
		if(invaddobj[i].checked)  invoiceaddress = invaddobj[i].value;
    }
	
	var paymenttype  ='';
	for (var i=pyobj.length-1;i>=0;i--)
	{
		if(pyobj[i].checked)  paymenttype = pyobj[i].value;
    }
	if(paymenttype=='') {alert('请选择付款方式');window.location.href="#div_consignee";return;}
