<script type="text/javascript">
//分批发货
function splititem(inquiry_id,part_id){
    var buynum = document.getElementById("all_buynum"+part_id).value;
	$.openPopupLayer({
	name: "splititem",
	url: "/inquiryorder/splititem/inquiry_id/"+inquiry_id+"/part_id/"+part_id+'/buynum/'+buynum
	});	 
}
//取消分批发货计划
function cancel_splititem(part_id,buynum){
     var html = '<input type="hidden" name="part_id" value="'+part_id+'"/>';
	 html +='<input type="hidden" name="buynum" id="buynum_'+part_id+'" value="'+buynum+'"/>';
	 html +='<input type="text" name="needs_time" style="width:100px;" readonly="readonly" onclick="WdatePicker({startDate:\'\',dateFmt:\'yyyy-MM-dd\',alwaysUseStartDate:true,lang:\'sc\'})" title="点击选择日期" />';
	$("#needs_time_span_"+part_id).html(html);
	$("#can_split_"+part_id).css("display","block");
}
//和并订单改变快递费
function shipoderfreight(obj){
    var split_ok = $("#split_ok").val()?true:false;
    var delivery_type_obj = document.getElementsByName("delivery_type");
	if(obj.checked){
		//金额总计
		$("#total_need_freight").css("display","none");
		$("#total_no_need_freight").css("display","block");
	    for (var i=delivery_type_obj.length-1;i>=0;i--)
	    {
		    if(delivery_type_obj[i].value==3)  delivery_type_obj[i].disabled = true;
        }
		//隐藏拆分发货
		if(!split_ok){
		   $('.can_split_span').hide();
		}
	}else{
		//金额总计
		$("#total_need_freight").css("display","block");
		$("#total_no_need_freight").css("display","none");
		for (var i=delivery_type_obj.length-1;i>=0;i--)
	    {
		    if(delivery_type_obj[i].value==3)  delivery_type_obj[i].disabled = false;
        }
	    //显示拆分发货
		if(!split_ok){
		   $('.can_split_span').show();
		}
	}
}
