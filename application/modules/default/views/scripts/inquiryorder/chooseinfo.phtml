		var invoicetype='';
		var invoicetype_obj = document.getElementsByName("invoicetype");
		for (var i=invoicetype_obj.length-1;i>=0;i--){
		   if(invoicetype_obj[i].checked)  invoicetype = invoicetype_obj[i].value;
        }
		if(invoicetype==2){
